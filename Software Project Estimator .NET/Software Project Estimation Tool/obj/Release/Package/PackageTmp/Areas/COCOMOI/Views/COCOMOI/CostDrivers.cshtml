    </header>

    

    <br />
    @using (Html.BeginForm())
    {


    <div class="row">
        <div class="col-lg-3">
            <div class="panel panel-default">
                <div class="panel-body">

                    <div style="color:#1E5472; text-align:center;"><h4><b>PRODUCT ATTRIBUTES</b></h4></div>
                    <hr />


                    <table class="table table-responsive table-borderless">

                        <tr>


                            <th> <h5 class=""><b> <i>Cost Drivers</i> </b></h5></th>
                            <th> <h5 class="text-center"><b><i>Weights</i></b></h5> </th>


                        </tr>

                        <tr>

                            <td class="text-small" title="Reliability">RELY</td>
                            <td class="inLine">
                                @Html.DropDownListFor(m => m.RELY_VALUE, Model.RELY, "Select", new { @class = "btn btn-default btn-sm" })
                                @Html.ValidationMessageFor(m => m.RELY_VALUE, "", new { @class = "text-error" })
                            </td>

                        </tr>

                        <tr>

                            <td class="text-small" title="Database size">DATA</td>
