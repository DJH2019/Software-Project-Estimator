@model Software_Project_Estimation_Tool.Areas.COCOMOI.Models.CostDriversModel
@{
    ViewBag.Title = "CostDrivers";
    Layout = "~/Views/Shared/_Layout.cshtml";

    //if (Session["Technique"] == null || Session["Stage"] == null)
    //{
    //    Response.Redirect("~/Home/Index");
    //}
}


<style>
    .panel-default {
        border-radius: 10px 10px 10px 10px;
    }

    .panel-body {
        color: #000000;
        padding:15px;
        height:390px;
        

    }

    .btn-sm {
        width:80%;
    }
     

</style>





<div class="jumbotron-m">


    <header>

